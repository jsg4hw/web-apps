<!-- loop through all  contact display contact full name and name of contact company -->
<h1> Contacts </h1>

<% contacts = Contact.all%>

<ul>
    <% for contact in contacts %>
       <% company_name = Company.find_by({"id" => contact["company_id"]})%> 
    <li> <%=contact["first_name"]%> <%=contact["last_name"]%>, <%=company_name["name"]%> </li>

    <%end%>
</ul>